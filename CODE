import time
import sys

def is_pronic(n):
    start = time.time()
    k = int(n**0.5)
    result = (k * (k + 1) == n)
    end = time.time()
    print("Time taken:", end - start, "seconds")
    return result

# Example Output
print("Is 20 pronic?", is_pronic(20))

output:
Time taken: 9.54e-07 seconds
Is 20 pronic? True
