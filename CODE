  import time
import sys

def is_abundant(n):
    start = time.time()
    total = 1
    for i in range(2, int(n**0.5) + 1):
        if n % i == 0:
            total += i
            if i != n // i:
                total += n // i
    result = total > n
    end = time.time()
    print("Time taken:", end - start, "seconds")
    return result

# Example Output
print("Is 12 abundant? ->", is_abundant(12))

output:  
Time taken: 2.38e-06 seconds
Is 12 abundant? -> True
