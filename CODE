import time
import sys

def prime_factors(n):
    start = time.time()
    factors = []
    
    # Remove factors of 2
    while n % 2 == 0:
        factors.append(2)
        n //= 2

    # Check odd factors
    i = 3
    while i * i <= n:
        while n % i == 0:
            factors.append(i)
            n //= i
        i += 2

    # Any remaining prime
    if n > 2:
        factors.append(n)

    end = time.time()
    print("Time taken:", end - start, "seconds")
    return factors

# Example Output
print("Prime factors of 84 =", prime_factors(84))

  output:
Time taken: 2.41e-06 seconds
Prime factors of 84 = [2, 2, 3, 7]
------------------------------------------------------------------  


#Week 4
  
import time
import sys

def count_distinct_prime_factors(n):
    start = time.time()
    count = 0
    
    # Check factor 2
    if n % 2 == 0:
        count += 1
        while n % 2 == 0:
            n //= 2

    # Check odd factors
    i = 3
    while i * i <= n:
        if n % i == 0:
            count += 1
            while n % i == 0:
                n //= i
        i += 2

    # Remaining number is prime
    if n > 1:
        count += 1

    end = time.time()
    print("Time taken:", end - start, "seconds")
    return count

# Example Output
print("Distinct prime factors of 84 =", count_distinct_prime_factors(84))

 output:
Time taken: 2.38e-06 seconds
Distinct prime factors of 84 = 3
-------------------------------------------------------------------
import time
import sys

def is_prime(num):
    if num < 2:
        return False
    if num in (2, 3):
        return True
    if num % 2 == 0:
        return False
    i = 3
    while i * i <= num:
        if num % i == 0:
            return False
        i += 2
    return True

def is_prime_power(n):
    start = time.time()
    if n <= 1:
        print("Time taken:", time.time() - start, "seconds")
        return False

    # Try all primes p
    for p in range(2, int(n**0.5) + 2):
        if is_prime(p):
            power = p
            while power <= n:
                if power == n:
                    end = time.time()
                    print("Time taken:", end - start, "seconds")
                    return True
                power *= p

    end = time.time()
    print("Time taken:", end - start, "seconds")
    return False

# Example Output
print("Is 27 a prime power?", is_prime_power(27))

output:
Time taken: 4.01e-06 seconds
Is 27 a prime power? True
