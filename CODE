 import time
import sys

def is_harshad(n):
    start = time.time()
    digit_sum = sum(map(int, str(n)))
    result = (n % digit_sum == 0)
    end = time.time()
    print("Time taken:", end - start, "seconds")
    return result

# Example Output
print("Is 18 a Harshad number?", is_harshad(18)) 

output:
Time taken: 7.63e-07 seconds
Is 18 a Harshad number? True
