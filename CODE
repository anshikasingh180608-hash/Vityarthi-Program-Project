import time
import sys
import math

def is_prime(x):
    if x < 2:
        return False
    if x in (2, 3):
        return True
    if x % 2 == 0:
        return False
    i = 3
    while i * i <= x:
        if x % i == 0:
            return False
        i += 2
    return True

def gcd(a, b):
    while b:
        a, b = b, a % b
    return a

def is_carmichael(n):
    start = time.time()

    if n < 2 or is_prime(n):
        print("Time taken:", time.time() - start, "seconds")
        return False

    for a in range(2, n):
        if gcd(a, n) == 1:
            if pow(a, n - 1, n) != 1:
                end = time.time()
                print("Time taken:", end - start, "seconds")
                return False

    end = time.time()
    print("Time taken:", end - start, "seconds")
    return True

# Example Output
print("Is 561 a Carmichael number?", is_carmichael(561))  


output:
Time taken: 4.74e-05 seconds
Is 561 a Carmichael number? True
