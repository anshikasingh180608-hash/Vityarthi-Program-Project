  import time
import sys

def mean_of_digits(n):
    start = time.time()
    digits = list(map(int, str(n)))
    result = sum(digits) / len(digits)
    end = time.time()
    print("Time taken:", end - start, "seconds")
    return result

# Example Output
print("Mean of digits of 12345 =", mean_of_digits(12345))
  
output:
Time taken: 1.19e-06 seconds
Mean of digits of 12345 = 3.0
