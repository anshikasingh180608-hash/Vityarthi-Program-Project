 import time
import sys

def multiplicative_persistence(n):
    start = time.time()
    steps = 0

    while n >= 10:
        product = 1
        for digit in str(n):
            product *= int(digit)
        n = product
        steps += 1

    end = time.time()
    print("Time taken:", end - start, "seconds")
    return steps

# Example Output
print("Multiplicative persistence of 39 =", multiplicative_persistence(39))

output:
Time taken: 1.67e-06 seconds
Multiplicative persistence of 39 = 3
