import time
import sys

def lucas_sequence(n):
    start = time.time()

    if n <= 0:
        print("Time taken:", time.time() - start, "seconds")
        return []
    if n == 1:
        print("Time taken:", time.time() - start, "seconds")
        return [2]

    seq = [2, 1]

  for i in range(2, n):
        seq.append(seq[-1] + seq[-2])

    end = time.time()
    print("Time taken:", end - start, "seconds")
    return seq

# Example Output
print("Lucas sequence for n=10:", lucas_sequence(10))  


output:
Time taken: 2.05e-06 seconds
Lucas sequence for n=10: [2, 1, 3, 4, 7, 11, 18, 29, 47, 76]
