import time
import sys

def collatz_length(n):
    start = time.time()
    steps = 0

    while n != 1:
        if n % 2 == 0:
            n //= 2
        else:
            n = 3 * n + 1
        steps += 1

    end = time.time()
    print("Time taken:", end - start, "seconds")
    return steps

# Example Output
print("Collatz length of 19 =", collatz_length(19))

output:
Time taken: 6.8e-07 seconds
Collatz length of 19 = 20
-----------------------------------------------------------------

import time
import sys

def polygonal_number(s, n):
    start = time.time()

    value = ((s - 2) * n * n - (s - 4) * n) // 2

    end = time.time()
    print("Time taken:", end - start, "seconds")
    return value

# Example Output
print("5th hexagonal number:", polygonal_number(6, 5))

 output:
Time taken: 3.02e-07 seconds
5th hexagonal number: 45
