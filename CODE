  import time
import sys

def aliquot_sum(n):
    start = time.time()

    if n == 1:
        print("Time taken:", time.time() - start, "seconds")
        return 0

    total = 1
    i = 2
    while i * i <= n:
        if n % i == 0:
            total += i
            if i != n // i:
                total += n // i
        i += 1

    end = time.time()
    print("Time taken:", end - start, "seconds")
    return total

# Example Output
print("Aliquot sum of 12 =", aliquot_sum(12))


 output:
Time taken: 1.91e-06 seconds
Aliquot sum of 12 = 16
