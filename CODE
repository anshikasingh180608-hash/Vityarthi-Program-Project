import time
import sys

def count_divisors(n):
    div = 0
    i = 1
    while i * i <= n:
        if n % i == 0:
            if i * i == n:
                div += 1
            else:
                div += 2
        i += 1
    return div

def is_highly_composite(n):
    start = time.time()
    d_n = count_divisors(n)

    for i in range(1, n):
        if count_divisors(i) >= d_n:
            print("Time taken:", time.time() - start, "seconds")
            return False

    end = time.time()
    print("Time taken:", end - start, "seconds")
    return True

# Example Output
print("Is 12 highly composite?", is_highly_composite(12))

output:
Time taken: 3.52e-05 seconds
Is 12 highly composite? True
