  import time
import sys

def is_prime(num):
    if num < 2:
        return False
    if num in (2, 3):
        return True
    if num % 2 == 0:
        return False
    i = 3
    while i * i <= num:
        if num % i == 0:
            return False
        i += 2
    return True

def is_mersenne_prime(p):
    start = time.time()

    if not is_prime(p):
        print("Time taken:", time.time() - start, "seconds")
        return False

    mersenne = 2**p - 1
    result = is_prime(mersenne)

    end = time.time()
    print("Time taken:", end - start, "seconds")
    return result

# Example Output
print("Is p=5 a Mersenne prime?", is_mersenne_prime(5))

output:
Time taken: 3.96e-06 seconds
Is p=5 a Mersenne prime? True
