  import time
import sys
import math

def is_perfect_power(n):
    start = time.time()

    if n <= 1:
        print("Time taken:", time.time() - start, "seconds")
        return False

    for b in range(2, int(math.log2(n)) + 2):
        a = round(n ** (1 / b))
        if a > 1 and a ** b == n:
            end = time.time()
            print("Time taken:", end - start, "seconds")
            return True

    end = time.time()
    print("Time taken:", end - start, "seconds")
    return False

# Example Output
print("Is 64 a perfect power?", is_perfect_power(64))

output:
Time taken: 1.14e-06 seconds
Is 64 a perfect power? True
