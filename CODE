import time
import sys

def is_prime(num):
    if num < 2:
        return False
    if num in (2, 3):
        return True
    if num % 2 == 0:
        return False
    i = 3
    while i * i <= num:
        if num % i == 0:
            return False
        i += 2
    return True

def twin_primes(limit):
    start = time.time()
    
    pairs = []
    for p in range(2, limit + 1):
        if is_prime(p) and is_prime(p + 2):
            pairs.append((p, p + 2))
    
    end = time.time()
    print("Time taken:", end - start, "seconds")
    return pairs

# Example Output
print("Twin primes up to 50:", twin_primes(50))


 output:
Time taken: 1.25e-05 seconds
Twin primes up to 50: [(3, 5), (5, 7), (11, 13), (17, 19), (29, 31)]
