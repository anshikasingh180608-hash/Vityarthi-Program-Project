  import time
import sys

def mod_inverse(a, m):
    start = time.time()
    a = a % m
    for x in range(1, m):
        if (a * x) % m == 1:
            end = time.time()
            print("Time taken:", end - start, "seconds")
            return x
    end = time.time()
    print("Time taken:", end - start, "seconds")
    return None  # No inverse exists

# Example Output
print("mod_inverse(3, 11) =", mod_inverse(3, 11))

output:
Time taken: 1.19e-06 seconds
mod_inverse(3, 11) = 4
-----------------------------------------------------------------
import time
import sys

def mod_inverse(a, m):
    for x in range(1, m):
        if (a * x) % m == 1:
            return x
    return None

def crt(remainders, moduli):
    start = time.time()
    M = 1
    for m in moduli:
        M *= m

    x = 0
    for r, m in zip(remainders, moduli):
        Mi = M // m
        inv = mod_inverse(Mi % m, m)
        x += r * Mi * inv

    end = time.time()
    print("Time taken:", end - start, "seconds")
    return x % M

# Example Output
print("CRT solution =", crt([2, 3, 2], [3, 5, 7]))

output:
Time taken: 3.8e-06 seconds
CRT solution = 23
----------------------------------------------------------------

  import time
import sys

def is_quadratic_residue(a, p):
    start = time.time()

    # Euler's criterion: a^((p-1)/2) ≡ 1 (mod p)
    result = pow(a, (p - 1) // 2, p) == 1

    end = time.time()
    print("Time taken:", end - start, "seconds")
    return result

# Example Output
print("Is 10 a quadratic residue mod 13?", is_quadratic_residue(10, 13))


output:
Time taken: 6.2e-07 seconds
Is 10 a quadratic residue mod 13? False
