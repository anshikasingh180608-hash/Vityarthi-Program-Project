import time
import sys

def mod_exp(base, exponent, modulus):
    start = time.time()
    result = 1
    base = base % modulus

    while exponent > 0:
        if exponent % 2 == 1:
            result = (result * base) % modulus
        base = (base * base) % modulus
        exponent //= 2

    end = time.time()
    print("Time taken:", end - start, "seconds")
    return result

# Example Output
print("mod_exp(2, 10, 1000) =", mod_exp(2, 10, 1000))

output:
Time taken: 7.15e-07 seconds
mod_exp(2, 10, 1000) = 24
