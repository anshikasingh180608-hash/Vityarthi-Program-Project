  import time
import sys

def is_automorphic(n):
    start = time.time()
    sq = n * n
    result = str(sq).endswith(str(n))
    end = time.time()
    print("Time taken:", end - start, "seconds")
    return result

# Example Output
print("Is 76 automorphic?", is_automorphic(76))

output:
Time taken: 1.05e-06 seconds
Is 76 automorphic? True
