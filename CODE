  import time
import sys

def digital_root(n):
    start = time.time()
    while n > 9:
        n = sum(map(int, str(n)))
    end = time.time()
    print("Time taken:", end - start, "seconds")
    return n

# Example Output
print("Digital root of 9875 =", digital_root(9875))

output:  
Time taken: 1.43e-06 seconds
Digital root of 9875 = 2
