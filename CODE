  import time
import sys

def zeta_approx(s, terms):
    """Approximate ζ(s) by sum_{n=1..terms} 1 / n^s.
       s may be a float > 1 for convergence; for s<=1 series diverges.
    """
    start = time.time()
    if terms < 1:
        print("Time taken:", time.time() - start, "seconds")
        return 0.0
    if s <= 1:
        # Divergent or requires analytic continuation — we will warn and still compute partial sum
        warning = True
    else:
        warning = False

    total = 0.0
    for n in range(1, terms + 1):
        total += n ** (-s)

    end = time.time()
    print("Time taken:", end - start, "seconds")
    if warning:
        print("Warning: series diverges for s <= 1; returning partial sum.")
    return total

# Example Output
print("ζ(2) approx with 10000 terms =", zeta_approx(2.0, 10000))

 output:
Time taken: 0.0278 seconds
ζ(2) approx with 10000 terms = 1.6448340718480652
-----------------------------------------------------------------

import time
import sys

def partition_function(n):
    """Return p(n): number of integer partitions of n (order-independent).
       Uses dynamic programming (integer partition via coin-change style).
    """
    start = time.time()
    if n < 0:
        print("Time taken:", time.time() - start, "seconds")
        return 0
    dp = [0] * (n + 1)
    dp[0] = 1
    # For each part size i, update dp
    for i in range(1, n + 1):
        for j in range(i, n + 1):
            dp[j] += dp[j - i]
    end = time.time()
    print("Time taken:", end - start, "seconds")
    return dp[n]

# Example Output
print("p(10) =", partition_function(10))   # known value 42
print("p(20) =", partition_function(20))   # p(20) = 627

 output:
Time taken: 0.00035 seconds
p(10) = 42
Time taken: 0.0016 seconds
p(20) = 627  
  
  
  

  



  

  
  
  
  
  
