import time
import sys

def count_divisors(n):
    start = time.time()
    divisors = 0

    i = 1
    while i * i <= n:
        if n % i == 0:
            if i * i == n:
                divisors += 1
            else:
                divisors += 2
        i += 1

    end = time.time()
    print("Time taken:", end - start, "seconds")
    return divisors

# Example Output
print("Number of divisors of 36 =", count_divisors(36))


output:
Time taken: 1.91e-06 seconds
Number of divisors of 36 = 9
