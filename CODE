  import time
import sys

def aliquot_sum(n):
    total = 1
    if n == 1:
        return 0
    i = 2
    while i * i <= n:
        if n % i == 0:
            total += i
            if i != n // i:
                total += n // i
        i += 1
    return total

def are_amicable(a, b):
    start = time.time()
    result = (aliquot_sum(a) == b and aliquot_sum(b) == a)
    end = time.time()
    print("Time taken:", end - start, "seconds")
    return result

# Example Output
print("Are 220 and 284 amicable?", are_amicable(220, 284))

output:  
Time taken: 3.8e-06 seconds
Are 220 and 284 amicable? True
