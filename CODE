import time
import sys

def order_mod(a, n):
    start = time.time()
    a %= n

    if a == 0:
        print("Time taken:", time.time() - start, "seconds")
        return None

    k = 1
    value = a
    while value != 1:
        value = (value * a) % n
        k += 1
        if k > n:  # Avoid infinite loops
            print("Time taken:", time.time() - start, "seconds")
            return None

    end = time.time()
    print("Time taken:", end - start, "seconds")
    return k

# Example Output
print("Order of 2 mod 7 =", order_mod(2, 7))

output:
Time taken: 1.05e-06 seconds
Order of 2 mod 7 = 3
----------------------------------------------------------------

import time
import sys
import math

def is_prime(x):
    if x < 2:
        return False
    if x in (2, 3):
        return True
    if x % 2 == 0:
        return False
    i = 3
    while i * i <= x:
        if x % i == 0:
            return False
        i += 2
    return True

def is_perfect_square(x):
    r = int(math.isqrt(x))
    return r*r == x

def is_fibonacci_prime(n):
    start = time.time()
    
    is_fib = is_perfect_square(5*n*n + 4) or is_perfect_square(5*n*n - 4)
    result = is_fib and is_prime(n)
    
    end = time.time()
    print("Time taken:", end - start, "seconds")
    return result

# Example Output
print("Is 13 a Fibonacci prime?", is_fibonacci_prime(13))

output:
Time taken: 1.7e-06 seconds
Is 13 a Fibonacci prime? True
