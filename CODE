import time
import sys

def is_deficient(n):
    start = time.time()
    if n == 1:
        print("Time taken:", time.time() - start, "seconds")
        return True
    total = 1
    for i in range(2, int(n**0.5) + 1):
        if n % i == 0:
            total += i
            if i != n // i:
                total += n // i
    result = total < n
    end = time.time()
    print("Time taken:", end - start, "seconds")
    return result

# Example Output
print("Is 15 deficient?", is_deficient(15))

output:
Time taken: 2.15e-06 seconds
Is 15 deficient? True
